{
  "name": "express-view-switcher",
  "description": "View switcher for Express.js",
  "version": "1.0.2",
  "author": "shimataro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shimataro/express-view-switcher.git"
  },
  "keywords": [
    "express",
    "multi-language",
    "multi-device",
    "pug"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">=2.0.0"
  },
  "scripts": {
    "build": "npm run build-core",
    "build-core": "babel ./src --out-dir ./dist --source-maps",
    "watch": "npm run build-core -- --watch",
    "postinstall": "npm run build",
    "check-updates": "ncu"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "npm-check-updates": "2.13.0"
  },
  "dependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-plugin-add-module-exports": "0.3.3"
  }
}
